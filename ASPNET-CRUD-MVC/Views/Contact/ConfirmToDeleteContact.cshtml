@model ContactModel
@{
    ViewData["Title"] = "Delete contact";
}

<div class="text-center">
    <h3> Want to delete [@Model.Name] contact?</h3>

</div>
<a class="btn btn-primary btn-block" asp-route-id="@Model.Id" asp-controller="Contact" asp-action="DeleteContact"> Confirm delete</a>
<a class="btn btn-danger btn-block" asp-controller="Contact" asp-action="Index"> Cancel</a>